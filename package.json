{
  "name": "send-track-api",
  "version": "1.0.0",
  "description": "API to track all your expenses efficiently and effectively.",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/index.ts",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint": "eslint . --ext .ts",
    "start": "node dist/src/index.js",
    "test:integration": "jest tests/integration --verbose --detectOpenHandles",
    "test:unit": "jest tests/unit --passWithNoTests",
    "test": "tsc --project tsconfig.test.json --noEmit && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://matiasmorons@github.com/matiasmoron/send-track-api.git"
  },
  "author": "Matías Morón",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matiasmoron/send-track-api/issues"
  },
  "homepage": "https://github.com/matiasmoron/send-track-api#readme",
  "dependencies": {
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "colorette": "^2.0.20",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "jsonwebtoken": "9.0.2",
    "mysql2": "3.14.0",
    "openai": "4.91.1",
    "pg": "8.14.1",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.21"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@eslint/js": "9.23.0",
    "@types/bcrypt": "5.0.2",
    "@types/express": "5.0.1",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.9",
    "@types/node": "22.14.0",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.17.0",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-promise": "7.2.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "supertest": "7.1.0",
    "ts-jest": "29.3.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.28.0"
  }
}
